<html>
  <head>
    <style>
      table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
      }

      td, th {
        border: 1px solid #eee;
        text-align: left;
        padding: 8px;
      }

      tr:nth-child(even) {
        background-color: #eee;
      }
    </style>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <%- headings.each do |heading| -%>
          <th><%= heading %></th>
          <%- end -%>
        </tr>
      </thead>
      <tbody>
        <%- rows.each do |row| -%>
        <tr>
          <%- row.each do |col| -%>
          <td><%= col -%></td>
          <%- end -%>
        </tr>
        <%- end -%>
      </tbody>
    </table>
  </body>
</html>